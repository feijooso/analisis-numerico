)\n","      constantes_asintoticas[i] = (np.absolute(error_actual))/(np.absolute(error_previo) ** alpha)\n","\n","  return ordenes_convergencia, constantes_asintoticas"],"metadata":{"id":"va92BmSOYdf7","executionInfo":{"status":"ok","timestamp":1717850035811,"user_tz":180,"elapsed":10,"user":{"displayName":"Martina Panetta","userId":"03332279969740822995"}}},"execution_count":41,"outputs":[]},{"cell_type":"code","source":["alphas_b, lambdas_b = orden_convergencia([v for it,v in h_biseccion['p']], h_biseccion['dx'], i_b)\n","alphas_pf, lambdas_pf = orden_convergencia([v for it,v in h_pf['p']], h_pf['dx'], i_pf)\n","alphas_nr, lambdas_nr = orden_convergencia([v for it,v in h_nr['p']], h_nr['dx'], i_nr)\n","alphas_s, lambdas_s = orden_convergencia(h_secante['p'], h_secante['dx'], i_s)\n","alphas_nrm, lambdas_nrm = orden_convergencia(h_nr_mod['p'], h_nr_mod['dx'], i_nrm)"],"metadata":{"id":"T5aB4BVSYjgn","executionInfo":{"status":"ok","timestamp":1717850035812,"user_tz":180,"elapsed":11,"user":{"displayName":"Martina Panetta","userId":"03332279969740822995"}}},"execution_count":42,"outputs":[]},{"cell_type":"code","source":["def graficar(NR,secante,NRM,biseccion,punto_fijo,titulo,ylabel, log_scale=False):\n","  fig = plt.figure()\n","  ax = fig.add_subplot(1, 1, 1)\n","\n","  ax.plot(NR, label = 'Newton-Raphson', color = 'green', linewidth = 1, linestyle = '--')\n","  ax.plot(secante, label = 'Secante', color = 'red', linewidth = 1, linestyle = '--')\n","  ax.plot(NRM, label = 'Newton-Raphson Modificado', color = 'orange', linewidth = 1, linestyle = '--')\n","  ax.plot(biseccion, label = 'Biseccion', color = 'blue', linewidth = 1, linestyle = '--')\n","  ax.plot(punto_fijo, label = 'Punto Fijo', color = 'pink', linewidth = 1, linestyle = '--')\n","  if log_scale:\n","    ax.set_yscale('symlog')\n","  ax.legend()\n","  ax.grid()\n","  plt.title(titulo)\n","  plt.xlabel(\"Iteraciones\")\n","  plt.ylabel(ylabel)\n","  plt.show()"],"metadata":{"id":"34LqU_TYYlk9","execution